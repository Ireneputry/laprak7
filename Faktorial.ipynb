   "source": [
    "n = int(input(\"n =\"))\n",
    "\n",
    "def deret(a):\n",
    "    if a == 0 or a == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return a * deret(a-1)\n",
    "\n",
    "for i in range(n, 0, -1):\n",
    "    x = deret(i)\n",
    "\n",
    "    print(x, end=\" \")\n",
    "    for j in range(i, 0, -1):\n",
    "        print(j, end=\" \")\n",
    "    print()\n",
    "\n"
   ]
